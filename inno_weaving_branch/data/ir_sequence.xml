        <record id="seq_weaving_center_cost_center" model="ir.sequence">
            <field name="name">Weaving center cost center</field>
            <field name="code">mrp.cost.center.weaving.center</field>
            <field name="prefix">WCC/%(range_year)s/</field>
            <field name="padding">10</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_weaving_center_job_work" model="ir.sequence">
