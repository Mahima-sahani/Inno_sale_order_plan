<?xml version="1.0" encoding="UTF-8" ?>

<odoo>
    <record id="barcode_paper_format" model="report.paperformat">
        <field name="name">Barcode paper-format</field>
        <field name="default" eval="True" />
        <field name="format">custom</field>
        <field name="page_height">79</field>
        <field name="page_width">51</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">2.00</field>
        <field name="margin_bottom">2.00</field>
        <field name="margin_left">1.0</field>
        <field name="margin_right">1.0</field>
        <field name="header_line" eval="False" />
        <field name="header_spacing">35</field>
        <field name="dpi">125</field>
    </record>

    <record id="cost_center_paper_format" model="report.paperformat">
        <field name="name">Cost Center paper-format</field>
        <field name="default" eval="True" />
        <field name="format">custom</field>
        <field name="page_height">297</field>
        <field name="page_width">210</field>
        <field name="orientation">Landscape</field>
        <field name="margin_top">2.00</field>
        <field name="margin_bottom">2.00</field>
        <field name="margin_left">1.0</field>
        <field name="margin_right">1.0</field>
        <field name="header_line" eval="False" />
        <field name="header_spacing">35</field>
        <field name="dpi">125</field>
    </record>
</odoo>